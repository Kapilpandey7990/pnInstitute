{
    "version": 2,
    "builds": [
      {
        "src": "./Server.js",
        "use": "@vercel/node"
      },
      {
        "src": "./index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/Server.js"
      }
    ]
  }
  