{
    "version": 2,
    "builds": [
      {
        "src": "./Server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public_html/index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/Server.js"
      },
      {
      "src": "/(.*)",
      "dest": "/public_html/index.html"
    }
    ]
  }
  